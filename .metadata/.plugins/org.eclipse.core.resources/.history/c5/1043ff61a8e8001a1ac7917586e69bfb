package com.example.mapper;

public class practice {

	public static void main(String[] args) {
		
		int number[]={0,1,2,3,4,5};
		
		int max=number[0];
		int min=number[0];
		
		for(int i=0;i<number.length;i++) {
			if(max>number[i]) {
				max=number[i];
			}
		}
		
		System.out.println(max);
		
	}

}
